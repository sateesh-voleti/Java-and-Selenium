<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
<Properties>
 <Property name= "basePath">./logs</Property>
</Properties>
 <Appenders>
   <Console name = "Console">
     <PatternLayout pattern= "%d{HH:mm:ss:SSS} %-5level %c{1} -%msg%n"/>
     </Console>
 </Appenders>
  <Loogers>
   <Root level='trace'>
    <AppenderRef ref= "Console"/>
   </Root>
  </Loogers>
</Configuration>